        <item name="colorPrimaryDark">@color/amoled_dark_color_primary_dark</item>
        <item name="colorAccent">@color/amoled_dark_color_accent</item>
        <item name="android:windowBackground">@color/amoled_dark_color_window_background</item>
    </style>

    <style name="AppbarThemeAmoledDark" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
    </style>


